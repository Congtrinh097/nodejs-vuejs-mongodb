{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "max-len": ["error", 160],
        "no-unused-vars": [
            "warnings",
            {
                "ignoreRestSiblings": true,
                "destructuredArrayIgnorePattern": "[A-Z]",
                "caughtErrors": "none"
            }
        ]
    }
}
